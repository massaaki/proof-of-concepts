{
	"compilerOptions": {
		"outDir": "dist",
		"target": "es2019",
		"module": "commonjs",
		"lib": [
			"ES6",
			"DOM"
		],
		"baseUrl": ".",
		"sourceMap": true,
		"removeComments": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"strict": false
	},
	"include": [
		"src"
	],
	"exclude": []
}